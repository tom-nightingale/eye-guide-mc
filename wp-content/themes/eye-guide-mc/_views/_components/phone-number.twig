<a href="tel:{{options.default_phone_number}}" class="{{classes}}">
    {% if icon %}
        {{function('icon', 'phone', 'transform rotate-90 mr-1 text-secondary')}}
    {% endif %}

    {% if calltag %}
        {{options.prefix_phone_number}}
    {% endif %}
    {{options.default_phone_number}}
</a>